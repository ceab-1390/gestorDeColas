<div class="row">
    <h1 id="oficina" class="d-none"><%=oficina%></h1>
    <%servicios.forEach(servicio => {%>
        <div class="col-sm-3 mt-3">
            <div class="card bg-card-mov">
                <h3 class="text-center card-title text-white"><%=servicio.descripcion%></h3>
                <div class="card-body">
                    <h1 class="text-center text-white ticket" id="<%=servicio._id%>"></h1>
                </div>
            </div>
        </div>
    <%})%>

</div>
<div class="row mt-5">
    <div class="col-sm-3 text-center img-qr">
        <img class="img-qr" src="/public/img/Captura desde 2024-07-01 22-10-58.png" alt="">
    </div>
</div>